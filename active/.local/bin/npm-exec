#!/usr/bin/env node

var run = require('child_process').spawn;
var bin = run('npm', ['bin']);

bin.stdout.on('data', function (data) {
  console.log(data.toString());
});

bin.stderr.on('data', function (data) {
  console.error(data);
});

bin.on('close', function (code) {
  console.log('child process exited with code ' + code);
  process.exit(code);
});

